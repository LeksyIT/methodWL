h = 0.001
emps1 = 5
alpha1 = 2.5
emps2 = 2
alpha2 = 1
tpoints = np.arange(0, 30, h)
x_0 = 2
y_0 = 2
for i in range(1, 7):
    xpoints, ypoints = [], []
    r = np.array([x_0 + i, y_0 + i], float)
    for t in tpoints:
        xpoints.append(r[0])
        ypoints.append(r[1])
        r += rk4(r, t, h)
    np.save(f'1/xpoints_1_{i}', xpoints)
    np.save(f'1/ypoints_1_{i}', ypoints)
    plt.plot(xpoints, ypoints)
    plt.scatter(2, 2, color='orange', s=40, marker='o')